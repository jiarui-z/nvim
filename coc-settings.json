{
  "coc.preferences.extensionUpdateCheck": "daily",
  "list.previousKeymap": "<C-j>",
  "list.nextKeymap": "<C-l>",
  "coc.preferences.floatActions": true,
  "coc.preferences.formatOnSaveFiletypes": ["json", "javascript"],
  "codeLens.enable": true,
  "diagnostic.refreshOnInsertMode": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.width": 30,
  "explorer.git.showIgnored": true,
  "explorer.keyMappingMode": "none",
  "suggest.completionItemKindLabels": {
    "function": "\uf794",
    "method": "\uf6a6",
    "variable": "\uf71b",
    "constant": "\uf8ff",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\ufa52",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf668",
    "color": "\ue22b",
    "property": "\ufab6",
    "field": "\uf93d",
    "unit": "\uf475",
    "file": "\uf471",
    "value": "\uf8a3",
    "event": "\ufacd",
    "folder": "\uf115",
    "keyword": "\uf893",
    "snippet": "\uf64d",
    "operator": "\uf915",
    "reference": "\uf87a",
    "typeParameter": "\uf278",
    "default": "\uf29c"
  },
  "explorer.keyMappings.global": {
    "<tab>": "actionMenu",
    "?": "help",
    "q": "quit",
    "i": "nodePrev",
    "k": "nodeNext",
    "l": ["wait", "expandable?", "expand", "open"],
    "gl": ["wait", "expand:recursive"],
    "gj": ["wait", "collapse:recursive"],
    "o": ["wait", "expanded?", "collapse", "expand"],

    "h": "addFile",
    "M": "addDirectory",
    "cw": "rename",
    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "dD": "deleteForever",
    "p": "pasteFile",

    "f": "search",
    "F": "searchRecursive",

    "R": "refresh",
    "zh": "toggleHidden",

    "<<": "gitStage",
    ">>": "gitUnstage",

    "<2-LeftMouse>": [
      "expandable?",
      ["expanded?", "collapse", "expand"],
      "open"
    ]
  }
}
